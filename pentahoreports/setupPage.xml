<?xml version="1.0" encoding="UTF-8"?>
<dynaForm
  name="Pentaho Reports Setup Form"
  type="xmlform"
  labelwidth="200"
  width="600"
>

<TITLE1 type="title" group="1">
  <en>First Time Pentaho Setup Page</en>
</TITLE1>

<SUBTITLE1 type="subtitle" group="1">
  <en>Pentaho User Console</en>
</SUBTITLE1>

<PentahoServer type="text" maxlength="100" size ="60">
  <en>Pentaho Server</en>
</PentahoServer>

<PentahoSuperUsername type="text" maxlength="100" size ="60">
  <en>Pentaho Admin User Name</en>
</PentahoSuperUsername>

<PentahoSuperPassword type="text" maxlength="100" size ="60">
  <en>Password</en>
</PentahoSuperPassword>

<SUBTITLE2 type="subtitle" group="1">
  <en>Pentaho Administration Console</en>
</SUBTITLE2>

<PentahoAdmServer type="text" maxlength="100" size ="60">
  <en>Pentaho Administration Console</en>
</PentahoAdmServer>

<PentahoAdmUsername type="text" maxlength="100" size ="60">
  <en>Pentaho Administration User Name</en>
</PentahoAdmUsername>

<PentahoAdmPassword type="text" maxlength="100" size ="60">
  <en>Password</en>
</PentahoAdmPassword>

<PentahoPublishPassword type="text" maxlength="100" size ="60">
  <en>Pentaho Publisher Password</en>
</PentahoPublishPassword>

<ACCEPT type="submit" >
  <en>Save</en>
</ACCEPT>
</dynaForm>
        <model><![CDATA[solution:/{SOLUTION}/processmaker.mondrian.xml]]></model>  
        <showgrid><![CDATA[true]]></showgrid>  
        <charttype><![CDATA[4]]></charttype>  
        <chartlocation><![CDATA[bottom]]></chartlocation>  

        <chartshowslicer><![CDATA[false]]></chartshowslicer>  
        <connection>jdbc/{JNDI_SOLUTION}</connection>  
        <jndi>{JNDI_SOLUTION}</jndi>  
        <query><![CDATA[
select { [Measures].[Cases], [Measures].[Open Cases], [Measures].[Completed Cases]} ON COLUMNS,
  { [Process].Children } ON ROWS
from [cubo]

]]></query>  
